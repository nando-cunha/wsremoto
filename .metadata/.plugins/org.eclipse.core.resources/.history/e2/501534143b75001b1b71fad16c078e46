package variaveis;

public class TiposDados {
	
	public static void main(String[] args) {
		/*
		 * Alfanumérico: é um dado que você não utiliza em operações aritméticas.
		 * 
		 * Numérico: é um dado que pode ser utilizado em operações.
		 */
		String nome = "Fernando Cunha";
		int idade = 45;
		double altura = 1.84;
		System.out.print(nome);
		System.out.print(idade);
		System.out.print(altura);
		
		
		
		
		
	}
	
	
	
	
	
	
	
	
	
	
	
}
